{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/bin/Release/**/H.EFCore.Extensions.dll"
          ],
          "src": "../H.EFCore.Extensions"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "wiki/**.md",
          "wiki/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": "**/README.md"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [ "apispec/**.md", "apispec/**.yml" ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© 2022 hsin19.<br>Generated by <strong>DocFX</strong></span>",
      "_appTitle": "Documentation",
      "_enableSearch": true,
      "_enableNewTab": false,
      "_disableContribution": "true"
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "common",
        "diagrams",
        "footnotes"
      ]
    }
  }
}
